<apex:page controller="NewCaseListController">
    <apex:form >
        <table>
            <tr>
                <th>Case Number With Link</th>
                <th>Case Number</th>
            </tr>
            <apex:repeat value="{!newCases}" var="case">
            <tr>
                <td><apex:outputLink value="/{!case.Id}">{!case.CaseNumber}</apex:outputLink></td>
                <td>{!case.CaseNumber}</td>
            </tr>
            </apex:repeat>    
        </table>
        
    </apex:form>
</apex:page>